  <cd:ProductType name="UVN-1B-EARTH-NIR">
    <cd:Description>Sentinel-4 Level 1 NIR radiance</cd:Description>
    <cd:ProductDefinition id="UVN_1B_EARTH_NIR" format="hdf5" version="0">
      <cd:Description>The format definition for this product is taken from the netCDF4 file itself.</cd:Description>
      <cd:DetectionRule>
        <cd:MatchExpression>substr(0,31,filename())=="W_XX-EUMETSAT-Darmstadt,SND+SAT"</cd:MatchExpression>
        <cd:MatchExpression>substr(37,16,filename())=="UVN-1B-EARTH-NIR"</cd:MatchExpression>
      </cd:DetectionRule>
    </cd:ProductDefinition>
  </cd:ProductType>
