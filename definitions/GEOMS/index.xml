  <!-- version numbers are a combination of base format and template version: <format> + <version>
       format: hdf4=1000, hdf5=2000, netcdf3=3000 (netcdf4 is treated as hdf5) -->
  <cd:ProductType name="FTIR">
    <cd:Description>GEOMS template for FTIR</cd:Description>
    <cd:ProductDefinition id="FTIR_001_hdf4" format="hdf4" version="1001">
      <cd:Description>The format definition for this product is taken from the HDF4 file itself.</cd:Description>
      <cd:DetectionRule>
        <cd:MatchExpression path="/@DATA_TEMPLATE">str(.) == "GEOMS-TE-FTIR-001"</cd:MatchExpression>
      </cd:DetectionRule>
    </cd:ProductDefinition>
    <cd:ProductDefinition id="FTIR_002_hdf4" format="hdf4" version="1002">
