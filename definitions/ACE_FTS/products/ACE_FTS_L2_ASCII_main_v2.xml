  <cd:ProductVariable name="num_datalines">
    <!-- The array field_data has size (total filesize - start of array field_data) / (length one line in the array + sizeof(linefeed)) -->
    <cd:Init><![CDATA[$num_datalines = (filesize() - byteoffset(/field_data)) / (length(/field_data[0]) + 1)]]></cd:Init>
  </cd:ProductVariable>
